          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Set HAS_* flags">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">def at = vars.get(&apos;ACCESS_TOKEN&apos;); def rt = vars.get(&apos;REFRESH_TOKEN&apos;); def uid = vars.get(&apos;USER_ID&apos;); vars.put(&apos;HAS_ACCESS_TOKEN&apos;, ((at != null &amp;&amp; at.trim().length() &gt; 0)).toString()); vars.put(&apos;HAS_USER_ID&apos;, ((uid != null &amp;&amp; uid.trim().length() &gt; 0)).toString()); def resp = prev.getResponseDataAsString(); def preview = (resp == null) ? &apos;null&apos; : resp.replaceAll(&apos;[\r\n]+&apos;,&apos; &apos;).substring(0, Math.min(160, resp.length())); log.info(&apos;Login extracted: access.len=&apos; + ((at==null)?0:at.length()) + &apos;, refresh.len=&apos; + ((rt==null)?0:rt.length()) + &apos;, userId=&apos; + (uid==null?&apos;null&apos;:uid) + &apos;, preview=&apos; + preview);</stringProp>
          </JSR223PostProcessor>
          <hashTree/>
        </hashTree>
