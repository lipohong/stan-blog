            <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Log Create Comment result">
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">def id = vars.get(&apos;COMMENT_ID&apos;); vars.put(&apos;HAS_COMMENT_ID&apos;, ((id != null &amp;&amp; id.trim().length() &gt; 0)).toString()); def code = prev.getResponseCode(); def msg = prev.getResponseMessage(); def body = prev.getResponseDataAsString(); log.info(&apos;CREATE comment code=&apos; + code + &apos;, id=&apos; + id + &apos;, msg=&apos; + msg + &apos;, respPreview=&apos; + ((body==null)? &apos;null&apos; : body.replaceAll(&apos;[\\r\\n]+&apos;,&apos; &apos;).substring(0, Math.min(160, body.length()))));</stringProp>
