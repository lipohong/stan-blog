                <stringProp name="script">def id = vars.get(&apos;COMMENT_ID&apos;); def code = prev.getResponseCode(); def msg = prev.getResponseMessage(); def url = prev.getUrlAsString(); def body = prev.getResponseDataAsString(); log.info(&apos;DELETE comment id=&apos; + id + &apos;, code=&apos; + code + &apos;, msg=&apos; + msg + &apos;, url=&apos; + url + &apos;, respBodyPreview=&apos; + ((body==null)? &apos;null&apos; : body.replaceAll(&apos;[\\r\\n]+&apos;,&apos; &apos;).substring(0, Math.min(120, body.length()))));</stringProp>
